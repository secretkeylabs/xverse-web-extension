{
  "name": "xverse-web-extension",
  "description": "A Bitcoin wallet for Web3",
  "version": "0.48.0",
  "private": true,
  "engines": {
    "node": "^18.18.2"
  },
  "scripts": {
    "build": "npm run clean && NODE_ENV=production node webpack/utils/build.js",
    "build-named": "npm run clean && NODE_ENV=production node scripts/build-named.js",
    "start": "node webpack/utils/devServer.js",
    "clean": "rimraf build",
    "style": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "e2etest": "playwright test -g \"\" --grep-invert \"#localexecution\"",
    "e2etest:ui": "playwright test --ui",
    "e2etest:smoketest": "playwright test --grep \"#smoketest\"",
    "e2etest:skipped": "playwright test --grep \"#localexecution\" --workers=1",
    "e2etest:report": "playwright show-report",
    "knip": "knip",
    "ts-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet"
  },
  "overrides": {
    "buffer": "6.0.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint",
      "tsc-files --noEmit src/styled.d.ts src/react-app-env.d.ts"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
